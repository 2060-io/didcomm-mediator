{
  "name": "didcomm-mediator",
  "version": "1.4.0",
  "description": "2060 DIDComm Mediator",
  "type": "module",
  "main": "build/index.js",
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "prettier": "prettier --ignore-path .prettierignore '**/*.+(js|json|ts|md|yml|yaml)'",
    "format": "pnpm prettier --write",
    "check-types": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "check-format": "pnpm prettier --list-different",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "pnpm lint && pnpm check-format",
    "build": "tsc",
    "start": "node ./build/index.js",
    "vdrproxy": "node ./build/vdrproxy/index.js",
    "test": "jest --verbose",
    "test:agent": "node --loader ts-node/esm ./test/Client-Agent/ClientAgent.ts",
    "postinstall": "patch-package",
    "prepare": "corepack enable"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2060-io/didcomm-mediator.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@2060.io/credo-ts-didcomm-shorten-url": "0.0.12",
    "@credo-ts/askar": "^0.6.1",
