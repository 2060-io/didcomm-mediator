    "@credo-ts/didcomm": "^0.6.1",
    "@credo-ts/didcomm-push-notifications": "^0.0.1",
    "@credo-ts/didcomm-transport-queue-postgres": "^0.0.1",
    "@credo-ts/node": "^0.6.1",
    "@hyperledger/anoncreds-shared": "^0.2.4",
    "@openwallet-foundation/askar-nodejs": "^0.4.3",
    "@openwallet-foundation/askar-shared": "^0.4.3",
    "@types/amqplib": "^0.10.4",
    "@types/pg": "^8.10.9",
    "amqplib": "^0.10.3",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "dotenv": "^17.2.3",
    "express": "^4.18.1",
    "firebase-admin": "^11.0.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.0.0",
    "tslog": "^4.9.1",
    "typescript": "~5.9.3",
    "ws": "^8.13.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/cross-spawn": "^6.0.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/node-fetch": "^2.6.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.5.13",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.8.0",
    "ts-jest": "^29.2.5"
  },
  "resolutions": {
